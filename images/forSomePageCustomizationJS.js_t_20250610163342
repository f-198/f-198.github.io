/* 
该文档被引用在，一级概览meta嵌套（一级概览mate嵌套）  
为一些一级概览页，做方便判断管理的，个性化定制
*/


/* 
判断页面<title></title>显示内容
*/
var forPageTitleO = {
  
  /* 
    targetChannelIdP,目标栏目id
    titleEleStrContentP,要显示的内容
  */
  forPageTitleFn:function(targetChannelIdStrP,titleEleStrContentP){
    var titleO = document.querySelectorAll('title')[0];
    switch(targetChannelIdStrP){
      //互动（归档）
      case '9796':
        titleO.textContent=titleEleStrContentP;
      break;
    }
  },

};



















